<script>
  // Years of Recognition: 1993 ‚Üí current year
  (function() {
    const startYear = 1993;
    const currentYear = new Date().getFullYear();
    const yearsEl = document.getElementById('years-count');
    if (yearsEl) yearsEl.textContent = (currentYear - startYear + 1).toString();
  })();

  // Papers Honored: count all award cards
  (function() {
    const totalCards = document.querySelectorAll('.timeline-container .award-card').length;
    const papersEl = document.getElementById('papers-count');
    if (papersEl) papersEl.textContent = totalCards.toString();
  })();

  // Make entire card clickable if the title contains a link
  document.querySelectorAll('.award-card .paper-title a').forEach(a => {
    const card = a.closest('.award-card');
    if (!card) return;
    card.style.cursor = 'pointer';
    card.addEventListener('click', (e) => {
      if (e.target.tagName.toLowerCase() !== 'a') a.click();
    });
  });
</script>

</body>
</html>
