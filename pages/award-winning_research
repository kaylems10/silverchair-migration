        <script>
        // Calculate years of recognition
        const startYear = 1993;
        const currentYear = new Date().getFullYear();
        const yearsEl = document.getElementById('years-count');
        if (yearsEl) yearsEl.textContent = (currentYear - startYear + 1).toString();

        // Count papers
        const totalCards = document.querySelectorAll('.timeline-container .paper-card, .timeline-container .impact-card').length;
        const papersEl = document.getElementById('papers-count');
        if (papersEl) papersEl.textContent = totalCards.toString();

        // Make cards clickable
        document.querySelectorAll('.paper-card .paper-title a, .impact-card .paper-title a').forEach(a => {
            const card = a.closest('.paper-card, .impact-card');
            if (!card) return;
            card.addEventListener('click', (e) => {
                if (e.target.tagName.toLowerCase() !== 'a') a.click();
            });
        });
    </script>
        </div>
    </body>
